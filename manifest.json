{
  "manifest_version": 2,
  "name": "Vinno",
  "version": "0.0.2",
  "page_action": {
    "default_popup": "src/html/popup.html",
    "default_title": "Let's annotate"
  },
  "offline_enabled": true,
  "permissions": [
    "activeTab",
    "geolocation",
    "storage",
    "https://www.google.ro/*",
    "https://stackoverflow.com/*",
    "https://tunein.com/radio/*",
    "https://www.youtube.com/*",
    "https://www.vimeo.com/*"
  ],
  "options_page": "src/html/options.html",
  "content_scripts": [
    {
      "matches": [
        "https://stackoverflow.com/*"
      ],
      "js": [
        "src/scripts/content-utils.js",
        "src/scripts/content-stackoverflow.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "src/scripts/content-utils.js",
        "src/scripts/dependenciesResolver.js",
        "src/scripts/content-youtube.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://tunein.com/radio/*"
      ],
      "js": [
        "src/scripts/content-utils.js",
        "src/scripts/dependenciesResolver.js",
        "src/scripts/content-tunein.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": [
        "https://vimeo.com/*"
      ],
      "js": [
        "src/scripts/content-utils.js",
        "src/scripts/dependenciesResolver.js",
        "src/scripts/content-vimeo.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["src/scripts/background.js"],
    "persistent": false
  }
}